{
  "name": "yiisoft/yii2-app-advanced",
  "description": "Yii 2 Advanced Project Template",
  "keywords": [
    "yii2",
    "framework",
    "advanced",
    "project template"
  ],
  "homepage": "http://www.yiiframework.com/",
  "type": "project",
  "license": "BSD-3-Clause",
  "support": {
    "issues": "https://github.com/yiisoft/yii2/issues?state=open",
    "forum": "http://www.yiiframework.com/forum/",
    "wiki": "http://www.yiiframework.com/wiki/",
    "irc": "irc://irc.freenode.net/yii",
    "source": "https://github.com/yiisoft/yii2"
  },
  "minimum-stability": "dev",
  "require": {
    "php": ">=5.4.0",
    "yiisoft/yii2": "~2.0.14",
    "yiisoft/yii2-bootstrap": "~2.0.0",
    "yiisoft/yii2-swiftmailer": "~2.0.0 || ~2.1.0",
    "2amigos/yii2-grid-view-library": "*",
    "dvizh/yii2-tree": "*",
    "dvizh/yii2-gallery": "*",
    "dvizh/yii2-promocode": "*",
    "dvizh/yii2-seo": "*",
    "dvizh/yii2-filter": "*",
    "dvizh/yii2-field": "*",
    "kartik-v/yii2-grid": "*",
    "dvizh/yii2-relations": "*",
    "voskobovich/yii2-many-many-behavior": "^3.0",
    "kartik-v/yii2-widget-select2": "*",
    "asofter/yii2-imperavi-redactor": "dev-master@dev",
    "zelenin/yii2-slug-behavior": "~1.5.1",
    "kartik-v/yii2-export": "@dev",
    "kartik-v/yii2-mpdf": "@dev",
    "yii2-starter-kit/yii2-file-kit": "@stable",
    "league/flysystem": "*",
    "nex/yii2-datepicker": "*",
    "udokmeci/yii2-phone-validator": "~1.0.1"
  },
  "require-dev": {
    "yiisoft/yii2-debug": "~2.1.0",
    "yiisoft/yii2-gii": "~2.1.0",
    "yiisoft/yii2-faker": "~2.0.0",
    "mdmsoft/yii2-admin": "2.x-dev",
    "codeception/base": "^2.4.0",
    "codeception/verify": "~0.3.1",
    "symfony/browser-kit": ">=2.7 <=4.2.4",
    "2amigos/yii2-editable-widget": "~1.0"
  },
  "config": {
    "process-timeout": 1800,
    "fxp-asset": {
      "enabled": false
    }
  },
  "suggest": {
    "2amigos/yii2-editable-widget": "Required for the EditableColumn"
  },
  "autoload": {
    "psr-4": {
      "dvizh\\shop\\": "common/modules/shop",
      "dvizh\\order\\": "common/modules/order",
      "dvizh\\cart\\": "common/modules/cart"
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ]
}
